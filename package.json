    "next": "latest",
    "querystring": "^0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
